generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model User {
  @@map("users")
  
  id          Int   @id @default(autoincrement())
  firebaseId  String @map("firebase_id")
  name        String
  createdAt   DateTime @default(now()) @map("created_at")
} 